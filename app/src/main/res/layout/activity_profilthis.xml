<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profilthis"
    android:orientation="vertical">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.google.android.material.appbar.AppBarLayout
                android:id="@+id/bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?android:attr/windowBackground"
                app:layout_constraintBottom_toTopOf="@+id/imageView2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#608A3933">

                    <TextView
                        android:id="@+id/tail_client_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/acme"
                        android:gravity="center"
                        android:text="Tailor      "
                        android:textColor="#D3FAFAFA"
                        android:textSize="30sp" />

                </androidx.appcompat.widget.Toolbar>
            </com.google.android.material.appbar.AppBarLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="285dp">
                <ImageView
                    android:id="@+id/imageView22"
                    android:layout_width="match_parent"
                    android:layout_height="223dp"
                    android:layout_marginBottom="57dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/backmanq"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <RelativeLayout
                    android:id="@+id/relativeLayoutt"
                    android:layout_width="match_parent"
                    android:layout_height="123dp"
                    android:layout_marginTop="163dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="126dp">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/tailor_client_image"
                            android:layout_width="129dp"
                            android:layout_height="122dp"
                            android:layout_marginStart="33dp"
                            android:layout_marginTop="2dp"
                            android:layout_marginEnd="250dp"
                            android:layout_marginBottom="33dp"
                            app:civ_border_color="#fff"
                            app:civ_border_width="6dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="1.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                        <TextView
                            android:id="@+id/tailor_client_username"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="84dp"
                            android:layout_marginBottom="32dp"
                            android:textSize="25dp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toEndOf="@+id/tailor_client_image"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.789" />


                        <ImageButton
                            android:id="@+id/like"
                            android:layout_width="60dp"
                            android:layout_height="55dp"
                            android:layout_marginStart="342dp"
                            android:layout_marginTop="80dp"
                            android:layout_marginEnd="31dp"
                            android:layout_marginBottom="40dp"
                            android:backgroundTint="#00000000"
                            android:scaleType="fitXY"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            tools:ignore="MissingConstraints" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </RelativeLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="4dp"
                android:background="#20000000" />



            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/constraintLayout22"
                        android:layout_width="wrap_content"
                        android:layout_height="131dp"
                        android:layout_marginTop="7dp"
                        android:layout_gravity="center"
                        android:background="@drawable/cadre"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/bio_tail_client"
                            android:layout_width="match_parent"
                            android:layout_height="49dp"
                            android:gravity="center"
                            android:padding="7dp"
                            android:textSize="14dp" />

                        <View
                            android:id="@+id/view"
                            android:layout_width="230dp"
                            android:layout_height="0.5dp"
                            android:layout_gravity="center"

                            android:background="#27000000" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView5"
                                android:layout_width="23dp"
                                android:layout_height="27dp"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="5dp"
                                android:src="@drawable/tel" />

                            <TextView
                                android:id="@+id/tailor_tell"
                                android:layout_width="wrap_content"
                                android:layout_height="18dp"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="5dp"
                                android:textSize="14dp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView7"
                                android:layout_width="27dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="18dp"
                                android:layout_marginTop="5dp"
                                android:src="@drawable/mail" />

                            <TextView
                                android:id="@+id/tailor_maill"
                                android:layout_width="wrap_content"
                                android:layout_height="21dp"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="10dp"
                                android:paddingRight="10dp"
                                android:textSize="14dp" />

                        </LinearLayout>
                    </LinearLayout>


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp">


                        <View
                            android:id="@+id/view44"
                            android:layout_width="27dp"
                            android:layout_height="0.1dp"
                            android:layout_marginTop="23dp"
                            android:layout_marginEnd="379dp"
                            android:layout_marginBottom="23dp"
                            android:background="#27000000"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="1.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                        <TextView
                            android:id="@+id/tail_photos"
                            style="?android:attr/buttonBarButtonStyle"
                            android:layout_width="79dp"
                            android:layout_height="40dp"
                            android:layout_marginEnd="303dp"
                            android:fontFamily="@font/acme"
                            android:text="Posts"
                            android:textColor="#85000000"
                            android:textSize="16sp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="1.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <View
                            android:id="@+id/view33"
                            android:layout_width="300dp"
                            android:layout_height="0.1dp"
                            android:layout_marginStart="109dp"
                            android:layout_marginTop="23dp"
                            android:layout_marginEnd="2dp"
                            android:layout_marginBottom="23dp"
                            android:background="#27000000"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.coordinatorlayout.widget.CoordinatorLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/RecyclerVieww"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            tools:ignore="MissingConstraints"
                            tools:layout_editor_absoluteX="41dp"
                            tools:layout_editor_absoluteY="577dp" />



                    </androidx.coordinatorlayout.widget.CoordinatorLayout>
                </LinearLayout>
            </androidx.core.widget.NestedScrollView>



        </LinearLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/addcommand"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:layout_marginStart="339dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:backgroundTint="#CD5C5C"
            android:src="@drawable/shopping"
            app:borderWidth="0dp"
            app:elevation="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:pressedTranslationZ="12dp"
            app:tint="#F2FFFFFF" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>